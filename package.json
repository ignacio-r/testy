{
  "name": "@pmoo/testy",
  "version": "6.1.0",
  "description": "A minimal testing framework, for educational purposes.",
  "homepage": "https://ngarbezza.github.io/testy/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngarbezza/testy"
  },
  "bugs": {
    "url": "https://github.com/ngarbezza/testy/issues"
  },
  "engines": {
    "node": ">= 12.*"
  },
  "main": "testy.js",
  "scripts": {
    "lint": "npx eslint@latest .",
    "lint:fix": "npx eslint@latest . --fix",
    "test": "bin/testy_cli.js",
    "test:coverage": "npx nyc@latest --reporter=lcov --reporter=text npm test",
    "see:coverage": "open coverage/lcov-report/index.html",
    "test:mutation": "npx stryker run",
    "see:mutation": "open reports/mutation/mutation.html",
    "see:dependencies": "npx madge -i testy-dependencies.png lib/ bin/"
  },
  "bin": "bin/testy_cli.js",
  "files": [
    "bin",
    "lib",
    "testy.js"
  ],
  "directories": {
    "lib": "./lib"
  },
  "keywords": [
    "unit-test",
    "testing",
    "tdd",
    "simple",
    "learning",
    "teaching",
    "oop"
  ],
  "author": "Nahuel Garbezza <n.garbezza@gmail.com>",
  "license": "MIT",
  "dependencies": {}
}
